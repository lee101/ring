{% for ring in thumbs %}
    <a href="{{ ring.url }}" title="{{ ring.title }}" target="_blank">
        <img alt="$ {{ ring.price }}" src="http://cdn.ring.nz/{{ zutils.urlencode(fixtures.getCompanyById(ring.company_id).name) }}/{{ ring.urltitle }}.jpg"
             onerror="APP.thumbFallback(this, '{{ ring.image }}')"/>
    </a>
{% endfor %}
